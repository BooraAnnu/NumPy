    "# A python program to create an array logspace()\n",
    "from numpy import *\n",
    "\n",
    "a= logspace(1, 4, 5)\n",
    "n= len(a)   # find te number of elements in a\n",
    "\n",
    "#repeat from 0 to n-1 items\n",
    "for i in range(n):\n",
    "    print(\"% 1f\" % a[i], end=\" \") \n"
   ]
